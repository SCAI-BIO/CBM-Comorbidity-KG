SET Citation = {"PubMed", "Nat Neurosci. 2021 Feb;24(2):168-175. doi: 10.1038/s41593-020-00758-5. ", "33257876"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"



SET Species = "9606"

SET Support = "Clinically documented COVID-19-associated neurological alterations included impaired consciousness (n=5), intraventricular hemorrhage (n=2), headache (n=2) and behavioral changes (n=2); acute cerebral ischemia was reported for 2 individuals, while neuropathological postmortem workup revealed acute infarcts in 6 individuals (Supplementary Table 2)."

path(DO:"COVID-19") pos path(HP:"Reduced consciousness/confusion")
path(DO:"COVID-19") pos path(HP:"Intraventricular hemorrhage")
path(DO:"COVID-19") pos path(MESH:Headache)
path(DO:"COVID-19") pos path(HP:"Behavioral abnormality")
path(DO:"COVID-19") pos path(HP:"Cerebral ischemia")
path(DO:"COVID-19") pos path(MESH:Infarction)

SET Support = "Coexisting conditions included diabetes mellitus (n=4), hypertension (n=21), cardiovascular disease (n=9), hyperlipidemia (n=2), chronic kidney diseasechronic kidney disease (n=2), prior stroke (n=6) and dementia (n=5) (Supplementary Table 1)."

path(DO:"COVID-19") -- path(DO:"diabetes mellitus")
path(DO:"COVID-19") -- path(DO:hypertension)
path(DO:"COVID-19") -- path(MESH:"Cardiovascular Diseases")
path(DO:"COVID-19") -- path(MESH:Hyperlipidemias)
path(DO:"COVID-19") -- path(DO:"chronic kidney disease")
path(DO:"COVID-19") -- path(MESH:Stroke)
path(DO:"COVID-19") -- path(DO:dementia)

SET Support = "Correspondingly, ACE2 was detectable in olfactory mucosa by means of immunohistochemistry, while we found no reliable ACE2 immunoreactivity in the parenchyma of the CNS, namely, in the olfactory bulb and medulla oblongata (Supplementary Fig. 1)."

SET BioAssay = "immunohistochemistry"
SET MeSHAnatomy = "Olfactory Mucosa"
path(DO:"COVID-19") -> p(HGNC:ACE2)
UNSET MeSHAnatomy
UNSET BioAssay



SET Support = "In the CNS, we found strong upregulation of human leukocyte antigen (HLA)-DR on microglia/macrophages, which were often arranged in so- called microglial nodules, in 13 of 25 individuals analyzed (Supplementary Table 2 and Supplementary Fig. 4b–f)."

SET MeSHAnatomy = {"Central Nervous System", "Microglia"}
path(DO:"COVID-19") -> surf(complex(GO:"MHC class II protein complex"))
UNSET MeSHAnatomy

SET Support = "A correlate of this HLA-DR-positive, presumably myeloid-driven inflammatory response was found in the CSF, where levels of inflammatory mediators such as interleukin (IL)-6, IL-18, CC-chemokine ligand 2 (CCL2) and soluble intercellular adhesion molecule-1 (sICAM-1) were found to be increased (Supplementary Fig. 4a)30."

SET MeSHAnatomy = "Cerebrospinal Fluid"
path(DO:"COVID-19") -> complex(GO:"MHC class II protein complex")
path(DO:"COVID-19") -> bp(GO:"inflammatory response")
path(DO:"COVID-19") pos p(HGNC:IL6)
path(DO:"COVID-19") pos p(HGNC:IL18)
path(DO:"COVID-19") pos p(HGNC:CCL2)
path(DO:"COVID-19") pos p(HGNC:ICAM1)
UNSET MeSHAnatomy

SET Support = "In line with recent clinical data demonstrating thromboembolic CNS events in a few individuals with COVID-19 (ref. 31), we found in 18% of the 33 individuals investigated (n=6 of 33) a histopathological correlate of microthrombosis and subsequent acute territorial brain infarcts (Fig. 6a–c and Supplementary Table 2)."

SET Disease = "COVID-19"
SET BioAssay = "immunofluorescent labeling"
path(DO:"COVID-19") -> path(MESH:Thromboembolism)
path(DO:"COVID-19") -> path(DO:"brain infarction")
UNSET BioAssay
UNSET Disease

SET Support = "As shown for the CNS, microthromboembolic events were also detectable in the olfactory mucosa in one individual."

SET MeSHAnatomy = "Olfactory Mucosa"
path(DO:"COVID-19") -> path(MESH:Thromboembolism)
