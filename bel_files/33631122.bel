SET Citation = {"PubMed", "Stem Cell Reports. 2021 Mar 9;16(3):437-445. doi: 10.1016/j.stemcr.2021.02.005.", "33631122"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"


SET Species = "9606"
SET Support = "We showed that camostat or EK1 peptide inhibited viral infection, confirming that pseudovirus entry is mediated by the spike protein and is dependent on TMPRSS2 function (Figure 2C). "
SET MeSHAnatomy = {"Lung", "Organoids"}
SET BioAssay = "luciferase enzyme activity assay"
a(CHEBI:camostat) -| path(DO:"COVID-19")
a(FIXME:"EK1 peptide") -| path(DO:"COVID-19")
a(FIXME:"EK1 peptide") -| act(p(UNIPROT:P0DTC2))

SET Support = "As expected, combination of camostat and EK1 peptide robustly inhibited viral infection in LORGs (Figure 2C)."
composite(a(CHEBI:camostat), a(FIXME:"EK1 peptide")) -| path(DO:"COVID-19")
UNSET MeSHAnatomy

SET Support = "Next, we compared these results with a lung epithelial cell line, Calu-3, and obtained similar results whereby EK1 peptide or TMPRSS2 inhibitors, camostat and nafamostat, repressed viral infection, which was further reduced when both classes of inhibitors were combined (Figure S1I)."
SET CellLine = "Calu-3 cell"
a(FIXME:"EK1 peptide") -| bp(GO:"viral genome replication")
a(CHEBI:camostat) -| bp(GO:"viral genome replication")
a(CHEBI:nafamostat) -| bp(GO:"viral genome replication")
composite(a(CHEBI:camostat), a(FIXME:"EK1 peptide"), a(CHEBI:nafamostat)) -| bp(GO:"viral genome replication")

SET Support = "We then tested the inhibitory activities of nafamostat in Calu-3 cells infected with a patient derived isolate of SARS-CoV-2, whereby our results showed that nafamostat significantly reduced SARS-CoV-2 infection (Figures S1J–S1K) as observed in LORGs."
SET MeSHAnatomy = {"Lung", "Organoids"}
a(CHEBI:nafamostat) -| path(DO:"COVID-19")
UNSET CellLine
UNSET BioAssay

SET Support = "To investigate physiological effects of virus infection, we examined the host response genes and we found that expression of a number of immuneregulatory genes including STAT1, STAT2, interferons (IFNs), and chemokines were upregulated in LORGs (Figure 2F)."
SET TimeLine = "72 h post infection"
SET BioAssay = "real-time PCR "
path(DO:"COVID-19") pos r(HGNC:STAT1)
path(DO:"COVID-19") pos r(HGNC:STAT2)
path(DO:"COVID-19") pos r(HGNC:IRF7)
path(DO:"COVID-19") pos r(HGNC:CCL5)
path(DO:"COVID-19") pos r(HGNC:CXCL10)
path(DO:"COVID-19") pos r(HGNC:MX1)
path(DO:"COVID-19") pos r(HGNC:IFNA1)
path(DO:"COVID-19") pos r(HGNC:IFNB1)
path(DO:"COVID-19") pos r(HGNC:IFNG)
path(DO:"COVID-19") pos r(HGNC:TNF)
path(DO:"COVID-19") pos r(HGNC:IL6)
path(DO:"COVID-19") pos r(HGNC:CXCL8)
UNSET TimeLine

SET Support = "Our results showed that CCL5, CXCL10, IFN-b, interleukin-6 (IL-6), and IL-8 were all upregulated >10-fold, and these findings are consistent with the data obtained from SARS-CoV-2 infection of lung epithelial cells (Blanco-Melo et al., 2020) and COVID-19 patients relative to healthy donors (Daamen et al., 2020). "
path(DO:"COVID-19") pos r(HGNC:CCL5)
path(DO:"COVID-19") pos r(HGNC:CXCL10)
path(DO:"COVID-19") pos r(HGNC:IFNB1)
path(DO:"COVID-19") pos r(HGNC:CXCL8)


SET Support = "Interestingly, our results also indicate that SARS-CoV-2 infection activates the inflammasome pathway because the key genes of this pathway including NLRP3, ASC, IL-18, and caspase-1 were upregulated (Figure 2G)."
path(DO:"COVID-19") pos r(HGNC:NLRP1)
path(DO:"COVID-19") pos r(HGNC:NLRP3)
path(DO:"COVID-19") pos r(HGNC:NLRC4)
path(DO:"COVID-19") pos r(HGNC:PYCARD)
path(DO:"COVID-19") pos r(HGNC:IL18)
path(DO:"COVID-19") pos r(HGNC:CASP1)
UNSET BioAssay
UNSET MeSHAnatomy


SET Support = "qRT-PCR results showed that ACE2 and TMPRSS2 are expressed in CORGs and in all three cell types analyzed including NPCs, neurons, and astrocytes (Figures 3B and 3C)."
SET MeSHAnatomy = {"Neurons", "Astrocytes"}
SET Cell = "neural progenitor cell"
path(DO:"COVID-19") neg r(HGNC:ACE2)
UNSET Cell
path(DO:"COVID-19") neg r(HGNC:TMPRSS2)
UNSET MeSHAnatomy


SET Support = "Our results showed that infection was more efficient in neurons than in NPCs (Figures 3F and 3G), an observation that correlates with the levels of ACE2 and TMPRSS2 expression in neurons and NPCs (Figures 3B and 3C)."
SET BioAssay = "real-time PCR "
path(DO:"COVID-19") pos r(HGNC:ACE2)
path(DO:"COVID-19") pos r(HGNC:TMPRSS2)
UNSET BioAssay
SET BioAssay = "immunofluorescent labeling"
path(DO:"COVID-19") pos p(HGNC:ACE2)
path(DO:"COVID-19") pos p(HGNC:TMPRSS2)
UNSET BioAssay


SET Support = "Our analyses showed that SARS-COV-2 infection upregulated genes involved in innate immunity including IL6, IFIT3, OAS2, TLR3, and TLR7, complement system, calpain pathways, CDK5, apoptosis, and RIPK1/3-regulated necroptosis pathways (Figures 3K, 3L, 3M, and S2J)."
SET MeSHAnatomy = {"Brain", "Organoids", "Neurons"}
SET BioAssay = "real-time PCR "
SET Disease = "COVID-19"
