##################################################################################
# Statements #
##################################################################################

SET Citation = {"PubMed", "Neuron. 2023 Apr 5;111(7):1086-1093.e2. doi: 10.1016/j.neuron.2022.12.029.", "36669485"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"


SET Species = "9606"
SET Support = "Dementia had the most replicated associations after multiple test correction, with six viral groupings showing significant results: viral encephalitis, other viral diseases, viral warts, all influenza, influenza and pneumonia, and viral pneumonia."
SET Ethnicity = "European"
path(DO:dementia) -- path(DO:"viral encephalitis")
path(DO:dementia) -- path(DO:"common wart")
path(DO:dementia) -- path(DO:influenza)
path(DO:dementia) -- path(DO:pneumonia)
path(DO:dementia) -- path(DO:"viral pneumonia")


SET Support = "Influenza and pneumonia were significantly associated with five of the six NDDs (AD, ALS, dementia, PD, and VAS) in FinnGen; all five were replicated using the cross-sectional data from UKB."
SET Cohort = "FinnGen"
path(DO:influenza) -- path(DO:"Alzheimer's disease")
path(DO:influenza) -- path(DO:"amyotrophic lateral sclerosis")
path(DO:influenza) -- path(DO:dementia)
path(DO:influenza) -- path(DO:"Parkinson's disease")
path(DO:influenza) -- path(DO:"vascular dementia")
path(DO:pneumonia) -- path(DO:"Alzheimer's disease")
path(DO:pneumonia) -- path(DO:"amyotrophic lateral sclerosis")
path(DO:pneumonia) -- path(DO:dementia)
path(DO:pneumonia) -- path(DO:"Parkinson's disease")
path(DO:pneumonia) -- path(DO:"vascular dementia")

SET Support = "Viral encephalitis and viral and other specified intestinal infections were significant and replicated for both AD and dementia; the varicella-zoster virus
was significant and replicated for both MS and VAS (as a note, varicella-zoster virus is referred to as Zoster [herpes zoster] in the FinnGen and UKB coding system)."
path(DO:"viral encephalitis") -- path(DO:"amyotrophic lateral sclerosis")
path(DO:"viral encephalitis") -- path(DO:dementia) 
SET MeSHAnatomy = "Intestines"
path(DO:"intestinal infectious disease") -- path(DO:"amyotrophic lateral sclerosis")
path(DO:"intestinal infectious disease") -- path(DO:dementia)
UNSET MeSHAnatomy
a(NCIT:"Varicella Zoster Infection") -- path(DO:"multiple sclerosis")
a(NCIT:"Varicella Zoster Infection") -- path(DO:"vascular dementia")


SET Support = "As a positive control, we replicated the previously published findings showing an association between a preceding EBV exposure and increased risk of MS in FinnGen (HR 26.5; CI 3.7–191.6; p = 0.001)."
path(MESH:"Epstein-Barr Virus Infections") -- path(DO:"multiple sclerosis")

SET Support = "At 1–5 years preceding NDD diagnosis, 15 replicated pairings had significant associations, with hazard ratios ranging from 2.24(PD and influenza and pneumonia) to 24.14 (dementia and viral encephalitis)."
path(DO:dementia) pos path(DO:"viral encephalitis")

SET Support = "For 100% of these pairings, the hazard ratio for developing an NDD after a viral exposure was higher than the hazard ratio for being infected with a virus after the diagnosis of an NDD."
path(MESH:"Epstein-Barr Virus Infections") -- path(DO:"neurodegenerative disease")


SET Support = "For both pairs (influenza/Alzheimer's and Bell's palsy/dementia), the HR was higher before NDD diagnosis."
path(DO:"influenza") pos path(DO:"Alzheimer's disease")
path(DO:"Bell's palsy") pos path(DO:dementia)


SET Support = "The HR for varicella-zoster is higher after MS diagnosis, although the confidence intervals do overlap"
path(DO:"multiple sclerosis") -- a(NCIT:"Varicella Zoster Infection")
UNSET Cohort
UNSET Ethnicity
UNSET Species
UNSET Section
UNSET PublicationStatus
UNSET PublicationType
UNSET Citation