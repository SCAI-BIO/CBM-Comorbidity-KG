p(HGNC:SDC1) -> tloc(p(HGNC:APP, frag("1_42")), fromLoc(GO:"extracellular region"), toLoc(GO:"intracellular"))
p(HGNC:SDC2) -> tloc(p(HGNC:APP, frag("1_42")), fromLoc(GO:"extracellular region"), toLoc(GO:"intracellular"))
