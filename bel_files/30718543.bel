p(HGNC:SDC4) -> tloc(p(HGNC:APP, frag("1_42")), fromLoc(GO:"extracellular region"), toLoc(GO:"intracellular"))
UNSET TimeLine

SET Support = "Among SDC transfectants, the neuron specific SDC3 internalized Aβ1–42 the most and Trf the least (Fig. 3a–d)."
SET BioAssay = {"fluorescent labeling method", "flow cytometry"}
p(HGNC:SDC3) -> tloc(p(HGNC:APP, frag("1_42")), fromLoc(GO:"extracellular region"), toLoc(GO:"intracellular"))
p(HGNC:SDC3) -| tloc(p(HGNC:TF), fromLoc(GO:"extracellular region"), toLoc(GO:"intracellular"))
UNSET BioAssay

SET Support = "Compared to WT K562 cells, extracellular attachment of Aβ1–42 was also increased on SDC3 and 4 transfectants (p<0.05 and p<0.01, respectively), while in case of Trf, extracellular attachment to SDC3 transfectants was significantly (p<0.01) reduced (Fig. 3c,d)."
SET BioAssay = "fluorescent labeling method"
p(HGNC:SDC3) -> complex(p(HGNC:APP, frag("1_42")), a(GO:"plasma membrane"))
