SET Citation = {"PubMed", "Insights Imaging. 2022 Jan 15;13(1):7. doi: 10.1186/s13244-021-01144-w.", "35032223"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"


SET Species = "9606"

SET Support = "All patients were very critically ill, with acute respiratory distress syndrome and mechanical ventilation, associated with acute kidney failure and refractory shock"
path(DO:"COVID-19") pos path(DO:"adult respiratory distress syndrome")
a(NCIT:"Mechanical Ventilation") -| path(DO:"COVID-19")
path(DO:"acute kidney failure") -- path(DO:"COVID-19")
path(FIXME:"refractory shock") -- path(DO:"COVID-19")

SET Support = "One patient had cortical–subcortical probably chronic infarcts with petechial hemorrhages (two large infarcts—right frontal and left parietal, and two smaller infarct—left cerebellar and right frontal) (Fig. 2)."
SET MeSHAnatomy = {"Brain", "Cerebellum"}
SET Cell = {"right frontal lobe", "left parietal lobe"}
path(DO:"COVID-19") -> path(DO:"brain infarction")
path(DO:"COVID-19") -> path(HP:Petechiae)
UNSET Cell
UNSET MeSHAnatomy

SET Support = "One patient had bilateral frontal and parietal subarachnoid hemorrhage, and signal abnormalities in both corticospinal tracts, bilateral basal ganglia, hippocampi and cortical/subcortical parietal bilaterally, and in the right frontal lobe (Fig. 3). "
SET Anatomy = {"frontal sulcus", "sulcus of parietal lobe"}                          
SET Technique = "computed tomography scan"
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
UNSET Anatomy
path(DO:"COVID-19") -> path(MESH:"Subarachnoid Hemorrhage")
SET Anatomy = {"corticospinal tract", "collection of basal ganglia", "hippocampal field", "right frontal lobe"}
path(DO:"COVID-19") -> path(HP:"Abnormal synaptic transmission")
UNSET Anatomy


SET Support = "Increased T2 and FLAIR signal associated with punctate hemorrhagic foci was identifed in the splenium of corpus callosum in patient 4 (Fig. 4c, d)."
SET Anatomy = "splenium of the corpus callosum"
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
UNSET Anatomy

SET Support = "Patient 5 had some nonspecific multifocal white matter abnormalities with increased T2 and FLAIR signal, and some punctate hemorrhagic foci in the right frontal white matter (Fig.  5a, b)."
SET MeSHAnatomy = "White Matter"
SET Cell = "white matter of frontal lobe"
path(DO:"COVID-19") -> path(HP:"Abnormality of the cerebral white matter")
UNSET Cell
UNSET MeSHAnatomy

SET Support = "Periventricular punctate hemorrhagic foci were noted in patient 7 (Fig. 5c, d)."
SET MeSHAnatomy = "Brain" 
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
UNSET MeSHAnatomy

SET Support = "One subject had difuse edema and patchy white matter focal abnormalities. This patient did not undergo cerebral sampling (family did not consent) (Fig. 4a, b)."
SET MeSHAnatomy = "White Matter"
path(DO:"COVID-19") -> path(DO:"brain edema")
path(DO:"COVID-19") -> path(HP:"Abnormality of the cerebral white matter")
UNSET MeSHAnatomy
UNSET Technique

SET Support = "Histological brain analysis showed reactive gliosis, congestion and cortical neuron eosinophilic degeneration, and axonal disruption in all six cases (Fig. 6)"
SET BioAssay = "immunohistochemistry"
SET MeSHAnatomy = {"Brain", "Cerebral Cortex"}
path(DO:"COVID-19") -> path(MESH:Gliosis)
path(DO:"COVID-19") -> a(NCIT:"Tissue Congestion")
path(DO:"COVID-19") -| deg(a(MESH:Eosinophils))
path(DO:"COVID-19") -> path(HP:"Axonal degeneration")
UNSET MeSHAnatomy

SET Support = "Other findings included edema (5 cases), discrete perivascular hemorrhages (5), cerebral small vessel disease (3), perivascular hemosiderin deposits (3), Alzheimer type II glia (3), abundant corpora amylacea (3), foci of ischemic area (1), foci of periventricular encephalitis (1), fibrin thrombi (1) and periventricular vascular ectasia (1) (Fig. 6)."
SET MeSHAnatomy = "Brain"
path(DO:"COVID-19") -> path(DO:"brain edema")
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
path(DO:"COVID-19") -> path(MESH:"Cerebral Small Vessel Diseases")
path(DO:"COVID-19") -> a(NCIT:"Hemosiderin Deposition")
path(DO:"COVID-19") -> a(NCIT:"Alzheimer Type II Astrocyte")
path(DO:"COVID-19") -> a(NCIT:"Corpora Amylacea")
path(DO:"COVID-19") -> path(DO:ischemia) 
path(DO:"COVID-19") -> path(DO:encephalitis)
path(DO:"COVID-19") -> bp(GO:"blood coagulation, fibrin clot formation")
path(DO:"COVID-19") -> a(NCIT:"Vascular Ectasia")
UNSET MeSHAnatomy
UNSET BioAssay


SET Support = "Other autopsy findings included typical COVID-19 pneumonia (diffuse alveolar damage, pneumocyte cytopathic changes, fibrin thrombi and increased megakaryocytes in septal vessels), acute tubular necrosis and visceral congestion."
path(DO:"COVID-19") -> path(DO:pneumonia)
path(DO:"COVID-19") -> path(FIXME:"diffuse alveolar damage")
SET Cell = "pneumocyte"
path(DO:"COVID-19") -> bp(MESH:"Cytopathogenic Effect, Viral")
UNSET Cell
path(DO:"COVID-19") -> bp(GO:"blood coagulation, fibrin clot formation") 
SET Custom_anatomy = "septal vessel"
path(DO:"COVID-19") -| a(MESH:Megakaryocytes)      
UNSET Custom_anatomy                                          
path(DO:"COVID-19") -> path(HP:"Acute tubular necrosis")
path(DO:"COVID-19") -> path(FIXME:"visceral congestion")
UNSET PublicationStatus
UNSET PublicationType
UNSET Section
UNSET Species
UNSET Citation