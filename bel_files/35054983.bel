SET Section = "Results"


SET Support = "Overexpression of ACE2 in 293T-ACE2 cells increased the internalization of the WT spike."

SET BioAssay = "flow cytometry"
SET Concentration = "50 nM"
SET SARS_CoV2_Variant = "wild type"
SET Custom_CellLine = "293T cell overexpressing ACE2"
p(HGNC:ACE2) -> tloc(p(UNIPROT:P0DTC2), fromLoc(GO:"extracellular region"), toLoc(GO:intracellular))
UNSET Custom_CellLine
UNSET SARS_CoV2_Variant

SET Support = "Compared to Delta-treated 293T cells, Delta uptake into 293T-ACE cells remained almost unchanged, suggesting that Delta's cellular entry is less dependent on ACE2 than WT spike's"

SET Custom_CellLine = "293T cell overexpressing ACE2"
SET SARS_CoV2_Variant = "Delta"
p(HGNC:ACE2) causesNoChange tloc(p(UNIPROT:P0DTC2, var("p.del156EF157"), var("p.T19R"), var("p.R158G"), var("p.L452R"), var("p.T478K"), var("p.D614G"), var("p.P681R"), var("p.D950N")), fromLoc(GO:"extracellular region"), toLoc(GO:intracellular))
UNSET SARS_CoV2_Variant
UNSET Custom_CellLine
UNSET BioAssay


SET Support = "Additionally, neither spike protein affected cellular viability at the applied concentration of 50 nM (Supplementary Figure S3)."

SET Custom_CellLine = {"293T cell", "293T cell overexpressing ACE2"}
SET SARS_CoV2_Variant = "wild type"
