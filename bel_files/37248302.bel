SET Citation = {"PubMed", "Nat Med. 2023 Jun;29(6):1448-1455. doi: 10.1038/s41591-023-02358-9. ", "37248302"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"


SET Species = "9606"

SET Support = "Positive results for serum IP/RT-QuIC for patients with PD in the first, second and external cohorts were 210/221 (95%), 34/34 (100%) and 15/20 (75%), respectively (Table 2 and Extended Data Table 4). "
SET Technique = "immunoprecipitation-based real-time quaking-induced conversion"
path(DO:"Parkinson's disease") -> p(HGNC:SNCA)


SET Support = "Seventeen of 26 patients with MSA parkinsonian variant (MSA-P) and eight of 13 patients with MSA cerebellar variant (MSA-C) were positive (Supplementary Table 6)."
path(FIXME:"Multiple system atrophy parkinsonian variant") -> p(HGNC:SNCA) 
path(FIXME:"Multiple system atrophy cerebellar variant") -> p(HGNC:SNCA) 


SET Support = "All PRKN-positive patients had negative IP/RT-QuIC results. "
p(HGNC:PRKN, var("?")) -| p(HGNC:SNCA)
UNSET Technique

SET Support = "The forming rates of IP/RT-QuIC results were significantly higher in patients with synucleinopathy than in controls or non-synucleinopathy patients (P<0.001), but the comparison among patients with PD, DLB and MSA did not reveal significant results (Fig. 1a)."
path(DO:synucleinopathy) -> bp(FIXME:"alpha-synuclein aggregation")


SET Support = "In the external cohort, two non-synucleinopathy patients had negative results, whereas the patient with PDD had positive results (Extended Data Table 2)."
path(FIXME:"Parkinson’s disease with dementia") -> p(HGNC:SNCA)


SET Support = "The forming rates correlated with Unified Parkinson’s Disease Rating Scale Part III (UPDRS-III) (P=0.028) and disease duration (P=0.0051) in PD"
SET MeSHDisease = "Parkinson Disease"
bp(FIXME:"alpha-synuclein aggregation") pos path(HP:"Poor motor coordination")

SET Support = "The results of Pearson’s chi-square tests showed a significant positive correlation between the results of the IP/RT-QuIC and delayed heart-to-mediastinum (H/M) ratio with 123I-metaiodobenzylguanidine (MIBG) cardiac scintigraphy in patients with PD (Supplementary Table 8a) but not in patients with MSA (Supplementary Table 8b)."
SET Technique = "cardiac scintigraphy"
bp(FIXME:"alpha-synuclein aggregation") pos a(FIXME:"delayed heart-to-mediastinum ratio")
bp(FIXME:"alpha-synuclein aggregation") pos path(DO:"congestive heart failure")
UNSET Technique
UNSET MeSHDisease


SET Support = "All amplified fibrils derived from patients with RBD with positive IP/RT-QuIC results had the LB diseases-type structure (Supplementary Fig. 2)."
SET BioAssay = "transmission electron microscopy"
SET MeSHDisease = "REM Sleep Behavior Disorder"
p(HGNC:SNCA) -> a(MESH:"Lewy Bodies")
UNSET MeSHDisease
UNSET BioAssay


SET Support = "Intriguingly, a super-resolution microscopy analysis revealed that intracellular inclusions created by the PD seeds, MSA seeds or DLB seeds tend to exhibit different morphological features, such as fibrous inclusions, dense-core inclusions in MSA and pale-core inclusions in DLB/PDD (Fig. 3a). PD seeds formed significantly more fibrous inclusions (P<0.0001), and DLB/PDD seeds formed significantly more pale inclusions (P<0.0001) (Fig. 3b,c)."
SET CellLine = "HEK293"
SET Technique = "Super resolution structured illumination microscopy"
SET MeSHDisease = "Parkinson Disease"
p(HGNC:SNCA, var("p.Ala53Thr")) -> a(FIXME:"fibrous inclusions")
UNSET MeSHDisease
SET Disease = "Lewy body dementia"
SET Custom_Disease = "Parkinson’s disease with dementia"
p(HGNC:SNCA, var("p.Ala53Thr")) -> a(FIXME:"pale inclusions")
UNSET Custom_Disease
UNSET Disease


SET Support = "This analysis confirmed that the fluorescence density of PD inclusions was significantly lower than that of DLB and MSA inclusions (Fig. 3d)."
SET Disease = "Lewy body dementia"
SET MeSHDisease = "Multiple System Atrophy"
p(HGNC:SNCA, var("p.Ala53Thr")) -> a(GO:"inclusion body")
UNSET MeSHDisease
UNSET Disease
UNSET Technique
UNSET Species



SET Species = "10090"
SET Support = "MSA seed injection resulted in a more rapid development of phosphorylated α-synuclein pathology by 6 months than PD seed injection."
SET MeSHAnatomy = {"Brain", "Corpus Striatum"}
SET MeSHDisease = "Multiple System Atrophy"
SET Duration_after_injection = "6 months"
path(DO:"multiple system atrophy") -> p(MGI:Snca, pmod(Ph))
UNSET Duration_after_injection


SET Support = "Intriguingly, a reduction in the density of phosphorylated α-synuclein deposits was observed 1 year after MSA seed injection."
SET Duration_after_injection = "12 months"
path(MESH:"Multiple System Atrophy") -| p(MGI:Snca, pmod(Ph))


SET Support = "The significant loss of GST-pi-positive oligodendrocytes in mouse brains 1year after MSA seed injection (Extended Data Fig. 4) suggests that MSA seeds may induce oligodendrocyte degeneration."
p(MGI:Snca) -| a(MESH:Oligodendroglia)
UNSET MeSHDisease
UNSET Duration_after_injection

 
