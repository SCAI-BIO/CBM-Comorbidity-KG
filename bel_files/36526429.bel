SET Citation = {"PubMed", "Neurology. 2023 Mar 28;100(13):624-628.doi: 10.1212/WNL.0000000000201682.", "36526429"}

SET PublicationType = "Case Report"
SET PublicationStatus = "Published"
SET Section = "Results"

SET Support = "The perivascular region from the patient showed immune cell infiltrates, fibrinogen leakage, increased PECAM-1 expression, and CD68 infiltrates (Figure 1A)."
SET Species = "9606"
SET Gender = "Female"
SET BioAssay = "immunofluorescent labeling"
SET MeSHDisease = "Drug Resistant Epilepsy"
SET MeSHAnatomy = {"Brain", "Temporal Lobe"}
path(DO:"COVID-19") pos p(HGNC:PECAM1)
path(DO:"COVID-19") -> bp(FIXME:"fibrinogen leakage")
path(DO:"COVID-19") -> p(HGNC:CD68, loc(UBERON:"perivascular space"))

SET Support = "The perivascular region had astrocyte activation (Panel D)."
path(DO:"COVID-19") -> bp(GO:"astrocyte activation")
UNSET MeSHAnatomy

SET Support = "Numerous neurons were found to be surrounded by microglial cells in the brain of the patient, considered to represent neuronophagia (Panel E), and the number of the CD68+ microglial nodules was significantly increased compared with control tissues (Panel F)."
SET MeSHAnatomy = "Brain"
path(DO:"COVID-19") -> a(NCIT:Neuronophagia)
path(DO:"COVID-19") -> a(FIXME:"CD68+ microglial nodule")  
UNSET MeSHAnatomy

SET Support = "Panel G shows lymphocytic infiltrates (CD3+, CD4+, and CD8+) in the parenchyma (top row), perivascular region (middle row), and meninges (bottom row)."
SET MeSHAnatomy = "Brain"
path(DO:"COVID-19") -> a(MESH:"CD4-Positive T-Lymphocytes", loc(MESH:"Parenchymal Tissue"))
path(DO:"COVID-19") -> a(MESH:"CD4-Positive T-Lymphocytes", loc(UBERON:"perivascular space"))
path(DO:"COVID-19") -> a(MESH:"CD4-Positive T-Lymphocytes", loc(MESH:Meninges))
path(DO:"COVID-19") -> a(MESH:"CD8-Positive T-Lymphocytes", loc(MESH:"Parenchymal Tissue"))
path(DO:"COVID-19") -> a(MESH:"CD8-Positive T-Lymphocytes", loc(UBERON:"perivascular space"))
path(DO:"COVID-19") -> a(MESH:"CD8-Positive T-Lymphocytes", loc(MESH:Meninges))
path(DO:"COVID-19") -> a(FIXME:"CD3-positive T-lymphocyte", loc(MESH:"Parenchymal Tissue"))
path(DO:"COVID-19") -> a(FIXME:"CD3-positive T-lymphocyte", loc(UBERON:"perivascular space"))
path(DO:"COVID-19") -> a(FIXME:"CD3-positive T-lymphocyte", loc(MESH:Meninges))
UNSET MeSHAnatomy
UNSET BioAssay
UNSET Gender
UNSET MeSHDisease
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
