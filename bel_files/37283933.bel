SET Citation = {"PubMed", "Free Neuropathol. 2023 Feb 10:4:4-1. doi: 10.17879/freeneuropathology-2023-4584.", "37283933"}


SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"



SET Species = "9606"

SET Support = "As mentioned previously, radiological findings shared common features in the cases for whom MRI was available (Fig. 1, a-p). The same was true for neuropathology."
SET MeSHAnatomy = "Brain"
SET Technique = "magnetic resonance imaging"
path(DO:"COVID-19") -> path(MESH:"Cerebral Small Vessel Diseases")
path(DO:"COVID-19") -> path(FIXME:"microhemorrhagic foci")
UNSET Technique
UNSET MeSHAnatomy
       
SET Support = "In the biopsy samples, multiple well-delineated foci of CD163 positive macrophages were observed in the white matter (Fig. 1. q, t, u, v)."
SET MeSHAnatomy = {"Brain", "White Matter"}
SET Technique = {"magnetic resonance imaging", "biopsy"}
path(DO:"COVID-19") -> a(FIXME:"CD163 positive macrophage", loc(MESH:"White Matter"))
UNSET Technique
UNSET MeSHAnatomy



SET Support = "In the foci, numerous and large axonal swellings were seen and white matter was vacuolized (Fig. 1. x). "
SET MeSHAnatomy = {"Brain", "Axons"}
SET Technique = "magnetic resonance imaging"
path(DO:"COVID-19") -> path(DO:"neuroaxonal dystrophy")
UNSET MeSHAnatomy
SET MeSHAnatomy = "White Matter"
path(DO:"COVID-19") -> a(NCIT:Spongiosis)
UNSET MeSHAnatomy


SET Support = "At the periphery of the lesions, apoptotic nuclei and occasional glial nuclear alterations, reminiscent of Creutzfeldt-Peters cells, were seen (Fig. 1. y)."
path(DO:"COVID-19") -> bp(GO:"nuclear fragmentation involved in apoptotic nuclear change")
path(DO:"COVID-19") -> bp(GO:"glial cell apoptotic process")
UNSET Technique

SET Support = "Some recent blood extravasation (Fig. 1. r) and a few siderophages were observed."
SET MeSHAnatomy = "Brain"
SET Technique = "hematoxylin staining"
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
path(DO:"COVID-19") -> a(FIXME:siderophage)
UNSET Technique
UNSET MeSHAnatomy

SET Support = "Macroscopic examination of the three autopsied brains was characterized by multiple hemorrhages scattered in the deep and subcortical white matter. In case # 4 these were predominant in the supramarginal gyrus, first and second temporal gyri, where they were particularly numerous, and also in the pons and cerebellum. There was a 4 mm hemorrhage in the left occipital pole."
SET MeSHAnatomy = {"Brain", "White Matter", "Pons", "Cerebellum"}
SET Anatomy = "supramarginal gyrus" 
SET Custom_anatomy = {"first temporal gyrus", "second temporal gyrus"}
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
UNSET Anatomy
UNSET MeSHAnatomy
UNSET Custom_anatomy

SET Support = "In cases # 5 and # 6 the hemorrhages were predominant in the frontal lobe with additional bleeds in the striatum (case # 5) and in the pons (case # 6)."
SET MeSHAnatomy = {"Frontal Lobe", "Pons"}
SET Anatomy = "striatum"
path(DO:"COVID-19") -> path(MESH:Hemorrhage)
UNSET MeSHAnatomy
UNSET Anatomy

SET Support = "The most conspicuous lesions were small foci rich in CD163 positive macrophages centered by a small caliber arteriole (Fig. 2. a, b, c) associated with swollen axons and vacuolization of the surrounding white matter (Fig. 2. d, e)."
SET Technique = "hematoxylin-eosin staining"
SET MeSHAnatomy = "Axons"
path(DO:"COVID-19") -> a(FIXME:"CD163 positive macrophage")
path(DO:"COVID-19") -> path(DO:"neuroaxonal dystrophy")
UNSET MeSHAnatomy
SET MeSHAnatomy = {"Brain", "White Matter"}
path(DO:"COVID-19") -> a(NCIT:Spongiosis)
UNSET MeSHAnatomy

SET Support = "Compared to biopsy cases, perivascular bleeding was more frequent and of different age (Fig. 2. f, g)."
SET Anatomy = "perivascular space"
path(DO:"COVID-19") -> path(MESH:Hemorrhage)

SET Support = "Fibrinogen and IgG IHC were positive in the lesions, evidencing blood-brain barrier leakage (Fig. 2. h)."
path(DO:"COVID-19") -| bp(GO:"maintenance of permeability of blood-brain barrier")
UNSET Anatomy
UNSET Technique



SET Support = "AÎ² IHC was negative indicating absence of amyloid angiopathy. In summary, the lesions of the white matter can be described as CCM."
SET MeSHAnatomy = "White Matter"
SET BioAssay = "immunohistochemistry"
path(DO:"COVID-19") -> path(MESH:"Cerebral Small Vessel Diseases")
UNSET MeSHAnatomy
UNSET BioAssay

SET Support = "However at a short distance from the hemorrhagic foci, both antibodies labelled numerous inclusions in endothelial cells of apparently spared vessels (Fig. 3. a, b). "
path(DO:"COVID-19") -> p(UNIPROT:P0DTC2, loc(MESH:"Endothelial Cells"))
path(DO:"COVID-19") -> p(UNIPROT:P0DTC9, loc(MESH:"Endothelial Cells"))
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
UNSET Citation