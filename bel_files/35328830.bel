SET Citation = {"PubMed", "Int J Mol Sci. 2022 Mar 21;23(6):3407. doi: 10.3390/ijms23063407.","35328830"}
SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"

SET Support = "Both cell lines have endogenous SDC3 expression (Supplementary Figure S1), and incubating them with TNF-α for seven days increased their SDC3 expression."
SET Species = "10090"
SET CellLine = "SH-SY5Y cell"
SET Custom_CellLine = "hCMEC/d3 cell"
SET TimeLine = "7 days"
SET BioAssay = "flow cytometry"
p(MGI:Tnf) -> p(MGI:Sdc3)

SET Support = "SDC3 overexpression due to TNF-α was significantly increased in the two cell lines. The effect was most profound in SH-SY5Y cells and slightly less, still significant in HCMEC/D3 cells (Figure 1A–C)."
p(MGI:Tnf) -> p(MGI:Sdc3)
UNSET Custom_CellLine
UNSET CellLine
UNSET TimeLine
UNSET BioAssay

SET Support = "In our studies, 12-month-old APPSWE-Tau mice, compared with wild type (WT) C57BL/6 mice, exhibited significantly increased amyloid plaque load (Figure 2A–C)."
SET Mouse_Strain = {"STOCK Tg(APPSWE)2576Kha Tg(Prnp-MAPT*P301L)JNPL3Hlmc/Tac"}
SET Age = "12 months"
composite(p(MGI:Mapt, var("p.Pro301Lys")), p(MGI:App,var("?"))) -> path(MESH:"Plaque, Amyloid") 

SET Support = "Increased Aβ plaque load was also associated with elevated TNF-α concentrations in APPSWE-Tau mice’s brain samples, as shown by ELISA measurements (Figure 3A). "
SET BioAssay = "ELISA"
path(MESH:"Plaque, Amyloid") -> p(MGI:Tnf)

SET Support = "TNF-α blood concentrations of APPSWE-Tau were also significantly higher than WT controls (Figure 3B)."
SET MeSHAnatomy = {"Brain","Blood"}
composite(p(MGI:Mapt, var("p.Pro301Lys")), p(MGI:App,var("?"))) -> p(MGI:Tnf)
UNSET MeSHAnatomy

SET Support = "ELISA measurements also revealed increased SDC3 concentrations in the brain (Figure 4A) and the periphery, namely the liver (Figure 4B)."
SET MeSHAnatomy = {"Brain","Liver"}
composite(p(MGI:Mapt, var("p.Pro301Lys")), p(MGI:App,var("?"))) -> p(MGI:Sdc3)
UNSET MeSHAnatomy

SET Support = "As shown in Figure 4C,D, increased SDC3 concentrations in the brain and the liver correlated well with blood TNF-α concentrations, suggesting a cause and effect relationship between TNF-α and SDC3 expression, as shown in previous in vitro studies."
SET MeSHAnatomy = {"Brain","Liver","Blood"}
p(MGI:Sdc3) pos p(MGI:Tnf)
UNSET MeSHAnatomy
UNSET BioAssay

SET Support = "As Figure 5A–C shows that the SDC3 expression of PBECs isolated from APPSWE-Tau was significantly increased compared with those isolated from WT mice."
SET Custom_Cell = "Primary brain endothelial cell"
composite(p(MGI:Mapt, var("p.Pro301Lys")), p(MGI:App,var("?"))) -> p(MGI:Sdc3)
UNSET Custom_Cell

SET Support = "Monocytes isolated from APPSWE-Tau mice exhibited increased SDC3 expression than those from WT mice (Figure 6A–C)."
SET Cell = "monocyte"
SET BioAssay = "flow cytometry"
composite(p(MGI:Mapt, var("p.Pro301Lys")), p(MGI:App,var("?"))) -> p(MGI:Sdc3)
UNSET BioAssay

SET Support = "SDC3 expression of blood-derived monocytes showed a positive correlation (r = 0.81) with Aβ plaque deposition in the brain, showing that SDC3 on monocytes is a good indicator of amyloid pathology in the brain (Figure 6C)."
SET MeSHAnatomy = "Brain"
surf(p(MGI:Sdc3)) pos path(MESH:"Plaque, Amyloid") 
UNSET Cell
UNSET MeSHAnatomy
UNSET Age
UNSET Mouse_Strain
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
UNSET Section
UNSET Citation