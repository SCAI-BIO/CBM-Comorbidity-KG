SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"
SET Disease = "COVID-19"
SET Disease_Type = "Long COVID"
SET TimeLine = "3-4 months post infection"
SET Support = "The LC group was differentiated from controls by increased body temperature and CRP and lowered SpO2, Gpx and ANTIOX values."
