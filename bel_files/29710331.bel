# Document Annotations #
DEFINE ANNOTATION PublicationType AS LIST {"Review", "Research"}
DEFINE ANNOTATION PublicationStatus AS LIST {"Preprint", "Published"}
DEFINE ANNOTATION Section AS LIST {"Full Text", "Abstract", "Results"}

SET Citation = {"PubMed", "JAMA Neurol. 2018 Aug 1;75(8):939-946. doi:10.1001/jamaneurol.2018.0605.","29710331"}
SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"

SET Support = "We found a statistically significant 28% increase in the incidence of PD among patients with IBD compared with the unaffected matched controls, after adjusting for age and sex(Table 2,Figure)"
SET Species = "9606"
path(DO:"inflammatory bowel disease") pos path(DO:"Parkinson's disease") 

SET Support = "The increased rate of PD was observed equally among patients with CD (adjusted IRR, 1.26; 95% CI, 1.03-1.53;P = .02) and among those with UC (adjusted IRR, 1.31; 95% CI, 1.14-1.51; P < .001; Table 2)"

path(DO:"Crohn's disease") pos path(DO:"Parkinson's disease")
path(DO:"ulcerative colitis") pos path(DO:"Parkinson's disease")

SET Support = "Compared with non-IBD controls, a greater proportion of men with IBD developed PD (0.34% vs 0.25%)."
SET MeSHDisease = "Inflammatory Bowel Diseases"
SET Gender = "Male"
path(DO:"inflammatory bowel disease") pos path(DO:"Parkinson's disease") 
UNSET Gender
UNSET MeSHDisease

SET Support = "In addition, 13 089 patients with IBD were exposed to anti-TNF therapy on or after the IBD index date. We observed a markedly lower PD incidence rate (0.08 per 1000 patient-years)among patients with IBD who were exposed to anti-TNF therapy compared with patients with IBD who were not exposed to anti-TNF therapy (0.76 per 1000 patient-years)(Table 3)"
SET MeSHDisease = "Inflammatory Bowel Diseases"
p(HGNC:TNF) pos path(DO:"Parkinson's disease")
a(NCIT:"Anti-Tumor Necrosis Factor Therapy") -| path(DO:"Parkinson's disease")
UNSET MeSHDisease
UNSET Species
UNSET PublicationType
UNSET PublicationStatus
UNSET Section
UNSET Citation