SET Citation = {"PubMed", "Stem Cell Reports. 2022 Feb 8;17(2):307-320. doi: 10.1016/j.stemcr.2021.12.011. Epub 2022 Jan 20.", "35063125"}
SET PublicationType = "Research"
SET PublicationStatus = "Published"
SET Section = "Results"

SET Support = "Of note, IFITM1 and IFITM2, which are necessary for SARS-CoV-2 infection in human lung cells (Shi et al., 2021), were significantly upregulated (p = 0.03 for IFITM1, p = 0.01 for IFITM2) in COVID-19 patients."
SET BioAssay = "RNA-seq gene expression profiling assay"
SET Species = "9606"
SET Disease = "COVID-19"
SET MeSHAnatomy = "Brain"
SET Specimen = "post-mortem specimen"
path(DO:"COVID-19") pos r(HGNC:IFITM1)
path(DO:"COVID-19") pos r(HGNC:IFITM2)
UNSET BioAssay

SET Support = "Subsequent gene set enrichment analysis (GSEA) revealed an upregulation of mRNAs related to antigen processing and major histocompatibility complex (MHC) class I-dependent antigen presentation supporting the notion that SARS-CoV-2 directly affects brain endothelial cells."
SET BioAssay = "gene list enrichment analysis"
path(DO:"COVID-19") pos r(HGNC:"HLA-A")
path(DO:"COVID-19") pos r(HGNC:"HLA-B")
